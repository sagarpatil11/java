package com.addressbook;

public class Person 
{
	String firstname,lastname,address;
	long mobno;
	int zipcode;
	
	public Person(String firstname, String lastname, String address, long mobno, int zipcode) 
	{
		super();
		this.firstname = firstname;
		this.lastname = lastname;
		this.address = address;
		this.mobno = mobno;
		this.zipcode = zipcode;
	}
	
	
}
